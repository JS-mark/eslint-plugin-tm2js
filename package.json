{
  "name": "eslint-plugin-tm2js",
  "type": "module",
  "version": "2.0.5",
  "packageManager": "pnpm@8.14.1",
  "description": "Mark extended ESLint rules",
  "author": "The Mark <sunduo3195@qq.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/js-mark",
  "homepage": "https://github.com/js-mark/eslint-plugin-tm2js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-mark/eslint-plugin-tm2js.git"
  },
  "bugs": "https://github.com/js-mark/eslint-plugin-tm2js/issues",
  "keywords": [
    "eslint"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@tm2js/eslint-config": "^0.0.5",
    "@types/eslint": "^8.56.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.11.5",
    "@types/semver": "^7.5.6",
    "@typescript-eslint/rule-tester": "^6.19.0",
    "@typescript-eslint/typescript-estree": "^6.19.0",
    "@typescript-eslint/utils": "^6.19.0",
    "ajv": "^6.10.0",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "eslint-define-config": "^2.1.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.0",
    "lodash.merge": "4.6.2",
    "pnpm": "^8.14.1",
    "rimraf": "^5.0.5",
    "semver": "^7.5.4",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  },
  "resolutions": {
    "eslint-plugin-tm2js": "workspace:*"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
